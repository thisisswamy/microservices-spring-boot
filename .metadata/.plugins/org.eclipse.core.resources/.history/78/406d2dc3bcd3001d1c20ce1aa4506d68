package com.swamy.microservice.basics.models;

import org.springframework.stereotype.Component;

@Component
public class UserWithReviews {
	private UserResponse userResponse;
	private ReviewResponse movieResponse;
	public UserWithReviews() {
		super();
	}
	public UserResponse getUserResponse() {
		return userResponse;
	}
	public void setUserResponse(UserResponse userResponse) {
		this.userResponse = userResponse;
	}
	public ReviewResponse getMovieResponse() {
		return movieResponse;
	}
	public void setMovieResponse(ReviewResponse movieResponse) {
		this.movieResponse = movieResponse;
	}
	
	

}
